<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="tile" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TileVC">
   <children>
      <Pane>
         <children>
            <ImageView fx:id="imgTile" fitHeight="40.0" fitWidth="40.0" onMouseClicked="#rect_mouseClicked" pickOnBounds="true" preserveRatio="true" />
         	  <Rectangle fx:id="rectangle" arcHeight="5.0" fill="TRANSPARENT" height="40.0" onMouseEntered="#rect_mouseEntered" onMouseExited="#rect_mouseExited" stroke="TRANSPARENT" strokeType="INSIDE" width="40.0" />
         </children>
      </Pane>
   </children>
</StackPane>
